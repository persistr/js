{
  "name": "@persistr/js",
  "version": "3.10.4",
  "description": "Persistr client for Javascript",
  "keywords": [
    "CQRS",
    "eventsourcing",
    "eventstore",
    "event",
    "store",
    "sourcing",
    "client"
  ],
  "author": "Sasa Djolic <sasa.djolic@gmail.com> (https://www.linkedin.com/in/sasadjolic/)",
  "contributors": [
    "Sasa Djolic <sasa.djolic@gmail.com> (https://www.linkedin.com/in/sasadjolic/)"
  ],
  "homepage": "https://github.com/persistrdb/persistr-js",
  "bugs": "https://github.com/persistrdb/persistr-js/issues",
  "license": "LGPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "git@github.com:persistrdb/persistr-js.git"
  },
  "main": "./lib/persistr",
  "types": "./lib/persistr.d.ts",
  "directories": {
    "lib": "./lib"
  },
  "devDependencies": {
    "browserify-shim": "^3.8.14",
    "deep-equal": "^1.0.1",
    "mocha": "^6.1.4",
    "mustache": "^3.0.1",
    "uglifyify": "^5.0.1",
    "vows": "^0.8.2",
    "zombie": "^6.1.4"
  },
  "dependencies": {
    "app-root-path": "^3.0.0",
    "async-queue": "^0.1.0",
    "btoa": "^1.2.1",
    "cross-fetch": "^3.0.2",
    "delay": "^4.2.0",
    "dotenv": "^8.0.0",
    "highland": "^2.13.0",
    "http-status-codes": "^1.3.0",
    "lru-cache": "^5.1.1",
    "luxon": "^1.3.2",
    "matcher": "^2.0.0",
    "mysql": "^2.16.0",
    "promise-mysql": "^4.0.0",
    "pubnub": "^4.23.0",
    "scramjet": "^4.17.1",
    "tenacious-fetch": "^2.2.2",
    "tortoise": "^1.0.1",
    "uuid": "^3.3.2",
    "uuid-parse": "^1.0.0"
  },
  "engines": {
    "node": "^10.7.0",
    "npm": "^6.4.1"
  },
  "scripts": {
    "build-debug": "node build.js ; browserify lib/persistr.js -i tortoise > dist/persistr.js",
    "build": "node build.js ; browserify -g uglifyify lib/persistr.js -i tortoise | uglifyjs -c > dist/persistr.js",
    "repl": "node -e \"require('repl').start({ignoreUndefined: true})\" --experimental-repl-await",
    "test": "./node_modules/.bin/mocha --reporter spec",
    "annotation": "node -r dotenv/config tools/annotation.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browser": {
    "pubnub": "./vendors/pubnub.4.21.5.js"
  },
  "browserify-shim": {
    "PubNub": "pubnub"
  }
}
